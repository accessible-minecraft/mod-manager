{
    "app_name": "Mod Manager",
    "icon_path_darwin": "./resources/Icon.icns",
    "icon_path_linux": "./resources/Icon.png",
    "icon_path_windows": "./resources/Icon.ico",
    "environments": [
        {
            "arch": "amd64",
            "os": "darwin"
        },
        {
            "arch": "amd64",
            "os": "linux"
        },
        {
            "arch": "amd64",
            "os": "windows",
            "env": {
                "CC": "x86_64-w64-mingw32-gcc",
                "CXX": "x86_64-w64-mingw32-g++",
                "CGO_ENABLED": "1"
            }
        }
    ]
}